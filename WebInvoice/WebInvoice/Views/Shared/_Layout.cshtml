            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Нов документ</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </li>*@
                        <li class="nav-item dropdown">
